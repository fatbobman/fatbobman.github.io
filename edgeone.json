{
    "name": "blogsource",
    "outputDirectory": "./docs",
    "redirects": [
        {
            "source": "$wwwhost",
            "destination": "$host",
            "statusCode": 301
        }
    ],
    "rewrites": [
    ],
    "headers": [
        {
            "source": "/*",
            "headers": [
              {
                "key": "X-Frame-Options",
                "value": "DENY"
              },
              {
                "key": "Cache-Control",
                "value": "max-age=7200"
              }
            ]
          },
          {
            "source": "/images/*",
            "headers": [
              {
                "key": "Cache-Control",
                "value": "max-age=31536000"
              }
            ]
          },
          {
            "source": "/js/*",
            "headers": [
              {
                "key": "Cache-Control",
                "value": "max-age=31536000"
              }
            ]
          },
          {
            "source": "/rss/*",
            "headers": [
              {
                "key": "Cache-Control",
                "value": "max-age=31536000"
              }
            ]
          },
          {
            "source": "/style/*",
            "headers": [
              {
                "key": "Cache-Control",
                "value": "max-age=31536000"
              }
            ]
          }
    ]
}