{
    "name": "blogsource",
    "outputDirectory": "./docs",
    "redirects": [
        {
            "source": "$wwwhost",
            "destination": "$host",
            "statusCode": 301
        }
    ],
    "rewrites": [],
    "headers": [
        {
            "source": "/*",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "Cache-Control",
                    "value": "max-age=7200"
                }
            ]
        },
        {
            "source": "/_astro/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=15768000, immutable"
                }
            ]
        },
        {
            "source": "/rss/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=15768000, immutable"
                }
            ]
        }
    ]
}